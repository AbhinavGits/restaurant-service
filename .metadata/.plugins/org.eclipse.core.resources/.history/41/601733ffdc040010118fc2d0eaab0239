package com.abhinav.interview.coding.review;

import java.util.HashMap;
import java.util.List;
import java.util.Map;
import java.util.Set;
import java.util.stream.Collectors;

public class MapsExample {

	public static void main(String[] args) {
		
		Map<Integer,String> employees = new HashMap<Integer,String>(); 
		
		employees.put(1, "Abhinav");
		employees.put(2, "Swati");
		
		List<String> collect = employees.values().stream().collect(Collectors.toList());
		System.out.println(collect);
	
	}
}
