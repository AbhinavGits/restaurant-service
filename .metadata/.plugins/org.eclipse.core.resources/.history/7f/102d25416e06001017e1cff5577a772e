package com.abhinav.intrvw.coding;

import java.util.ArrayList;
import java.util.Comparator;
import java.util.List;
import java.util.stream.Collectors;

public class CodingExample1 {

	public static void main(String[] args) {
		
	// list of Students, print in opposite order of name
		
		
		List<Student> studentList = new ArrayList<Student>();
		studentList.add(new Student(1,"Abhinav"));
		studentList.add(new Student(2,"Bhavana"));
		studentList.add(new Student(3,"Chikram"));
		studentList.add(new Student(4,"Dhanno"));
		studentList.add(new Student(5,"Chameli"));
	//	studentList.forEach(s -> System.out.println(s));
		
		List<Student> sortedNames = studentList.stream().sorted(Comparator.comparing(Student::getName)).collect(Collectors.toList());
		sortedNames.forEach(s -> System.out.println(s));
	}

}

