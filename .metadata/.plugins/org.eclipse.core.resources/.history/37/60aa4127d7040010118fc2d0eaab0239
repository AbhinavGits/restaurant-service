package com.abhinav.interview.coding.review;

import java.util.Map;
import java.util.function.Function;
import java.util.stream.Collectors;

public class StringToFrequencyMap {
	
	public static void main(String[] args) {
		String str = "aAbBcCdD";
		Map<Character, Long> charMap = str.chars().mapToObj(ch -> (char) ch).collect(Collectors.groupingBy(Function.identity(),Collectors.counting()));

	}
	
	
}
