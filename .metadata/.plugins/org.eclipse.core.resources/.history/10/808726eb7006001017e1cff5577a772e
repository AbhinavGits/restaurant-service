package com.abhinav.intrvw.coding;

import java.util.ArrayList;
import java.util.Comparator;
import java.util.HashMap;
import java.util.List;
import java.util.Map;
import java.util.stream.Collectors;

public class CodingExample1 {

	public static void main(String[] args) {
		
	// list of Students, print in opposite order of name
		
		
		List<Student> studentList = new ArrayList<Student>();
		studentList.add(new Student(1,"Abhinav"));
		studentList.add(new Student(2,"Bhavana"));
		studentList.add(new Student(3,"Chikram"));
		studentList.add(new Student(4,"Dhanno"));
		studentList.add(new Student(5,"Chameli"));
	//	studentList.forEach(s -> System.out.println(s));
		
		List<Student> sortedNames = studentList.stream().sorted(Comparator.comparing(Student::getName).reversed()).collect(Collectors.toList());
		sortedNames.forEach(System.out::println);
		
		 Map<Integer, Student> studentMap = new HashMap<>();
	        studentMap.put(1, new Student(101, "Charlie"));
	        studentMap.put(2, new Student(102, "Alice"));
	        studentMap.put(3, new Student(103, "Bob"));
	}

}

