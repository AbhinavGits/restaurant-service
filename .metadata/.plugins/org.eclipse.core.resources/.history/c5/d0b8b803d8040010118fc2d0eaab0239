package com.abhinav.interview.coding.review;

import java.util.Arrays;
import java.util.List;
import java.util.function.Function;
import java.util.stream.Collectors;

public class ListOfStringsToFrequencyMap {

	public static void main(String[] args) {
		
		List<String> listOfStrings  = Arrays.asList("Abhinav","Swati");
		
		listOfStrings.stream().flatMapToInt(ch -> ch.chars()).mapToObj(ch -> (char) ch).collect(Collectors.groupingBy(Function.identity(),Collectors.counting()));
	}
}
